# Detect possible date and rate columns
date_candidates = [c for c in df.columns if 'date' in c.lower() or 'month' in c.lower() or 'year' in c.lower()]
rate_candidates = [c for c in df.columns if 'rate' in c.lower() or 'unemployment' in c.lower()]

print("Date candidates:", date_candidates)
print("Rate candidates:", rate_candidates)

# Rename them to standard names
if date_candidates:
    df = df.rename(columns={date_candidates[0]:'Date'})
if rate_candidates:
    df = df.rename(columns={rate_candidates[0]:'UnemploymentRate'})

